      "source": [
        "# compute IQ range to filter outliers \n",
        "q1 = t1['active_mins'].quantile(0.25) \n",
        "q3 = t1['active_mins'].quantile(0.75) \n",
        "print(q1,q3)\n",
        "iqr = q3 - q1\n",
        "cutoff = 1.5*iqr\n",
        "t1 = t1[((t1.active_mins)> q1 - cutoff)]\n",
        "t1 = t1[((t1.active_mins)< q3 + cutoff)]\n",
        "\n"
      ],
      "execution_count": 81,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
